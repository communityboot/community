<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.muchi.community.shiro.dao.UserDao">

    <select id="getUserByUserName" parameterType="string" resultType="com.muchi.community.base.entity.User">
        select * from sys_user where userName=#{userName}
    </select>



    <select id="userQuery" parameterType="string" resultType="com.muchi.community.base.entity.User">
        select * from sys_user
    </select>

</mapper>